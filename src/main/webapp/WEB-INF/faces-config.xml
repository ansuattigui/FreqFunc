<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <converter>
          <converter-for-class>java.lang.Enum</converter-for-class>
          <converter-class>javax.faces.convert.EnumConverter</converter-class>
        </converter>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>divisaoList</from-outcome>
            <to-view-id>/divisao/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>funcionarioList</from-outcome>
            <to-view-id>/funcionario/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>movimentoList</from-outcome>
            <to-view-id>/movimento/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userList</from-outcome>
            <to-view-id>/user/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>profileList</from-outcome>
            <to-view-id>/profile/List.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/divisao/List.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>divisaoEdit</from-outcome>
            <to-view-id>/divisao/Edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>divisaoCreate</from-outcome>
            <to-view-id>/divisao/Create.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>divisaoView</from-outcome>
            <to-view-id>/divisao/View.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/funcionario/List.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>funcionarioEdit</from-outcome>
            <to-view-id>/funcionario/Edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>funcionarioCreate</from-outcome>
            <to-view-id>/funcionario/Create.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>funcionarioView</from-outcome>
            <to-view-id>/funcionario/View.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>funcionarioDivisao</from-outcome>
            <to-view-id>/funcionario/SeletorDivisao.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/movimento/List.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>movimentoEdit</from-outcome>
            <to-view-id>/movimento/Edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>movimentoCreate</from-outcome>
            <to-view-id>/movimento/Create.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>movimentoView</from-outcome>
            <to-view-id>/movimento/View.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>movimentoDivisao</from-outcome>
            <to-view-id>/movimento/SeletorDivisao.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>movimentoFuncionario</from-outcome>
            <to-view-id>/movimento/SeletorFuncionario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/user/List.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>userEdit</from-outcome>
            <to-view-id>/user/Edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userCreate</from-outcome>
            <to-view-id>/users/Create.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>userView</from-outcome>
            <to-view-id>/users/View.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
